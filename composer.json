{
    "name": "assoconnect/log-bundle",
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/assoconnect/log-bundle/"
        }
    ],
    "type": "library",
    "description": "",
    "keywords": ["php", "Symfony", "log"],
    "homepage": "https://github.com/assoconnect/log-bundle/",
    "license": "MIT",
    "authors": [
        {
            "name": "AssoConnect",
            "homepage": "https://www.assoconnect.com"
        },
        {
            "name": "Florian GUIMIER"
        }
    ],
    "autoload": {
        "psr-4": {
            "AssoConnect\\LogBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AssoConnect\\LogBundle\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "symfony/yaml": "^5.0|^6.0",
        "symfony/validator": "^5.0|^6.0",
        "phpstan/phpstan-symfony": "^1.0",
        "assoconnect/php-quality-config": "^1.2"
    },
    "require": {
        "php": "^7.4|^8.2",
        "symfony/framework-bundle": "^5.0|^6.0",
        "doctrine/dbal": "^2.10|^3.0",
        "symfony/serializer": "^5.1|^6.0",
        "symfony/property-access": "^5.1|^6.0",
        "doctrine/doctrine-bundle": "^1.9|^2.0",
        "doctrine/orm": "^2.7",
        "moneyphp/money": "^3.0|^4.0",
        "ext-json": "*",
        "ramsey/uuid": "^3.7|^4.0",
        "ramsey/uuid-doctrine": "^1.4"
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
